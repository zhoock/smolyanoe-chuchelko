{
  "name": "my-project",
  "version": "1.0.0",
  "description": "Смоляное чучелко",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --env env=prod",
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check": "npm run stylelint && npm run lint && npm run format",
    "fix": "npm run stylelint:fix && npm run lint:fix && npm run format:fix",
    "prepare": "husky",
    "commit": "cz",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-loader": "^9.2.1",
    "commitizen": "^4.3.1",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^15.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.10",
    "prettier": "^3.6.2",
    "react-refresh": "^0.17.0",
    "sass": "^1.76.0",
    "sass-loader": "^14.2.1",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "glob": "^11.0.3",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.2.0",
    "react-router-dom": "^6.26.2",
    "rimraf": "^6.0.1"
  }
}
