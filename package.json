{
  "name": "my-project",
  "version": "1.1.0",
  "description": "Смоляное чучелко",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.cjs --env env=prod",
    "start": "webpack serve --config webpack/webpack.config.cjs --env env=dev",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check": "npm run stylelint && npm run lint && npm run format",
    "fix": "npm run stylelint:fix && npm run lint:fix && npm run format:fix",
    "prepare": "husky",
    "commit": "cz",
    "release": "standard-version",
    "test": "jest --verbose",
    "migrate": "tsx scripts/migrate-database.ts",
    "generate-encryption-key": "node scripts/generate-encryption-key.js",
    "test-db": "tsx scripts/test-database-connection.ts",
    "load-env": "echo '⚠️  Используйте: source scripts/load-netlify-env.sh' && bash scripts/load-netlify-env.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@netlify/functions": "^5.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@prettier/plugin-pug": "^3.0.0",
